<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>bzhcampdev</application>
    <version>v1</version>
    <threadsafe>true</threadsafe>
    <module>default</module>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
    <sessions-enabled>true</sessions-enabled>
    <use-google-connector-j>true</use-google-connector-j>
    <env-variables>
        <env-var name="ENV" value="${ENV}"/>
        <env-var name="GOOGLE_LOGIN" value="${GOOGLE_LOGIN}"/>
        <env-var name="GOOGLE_PASSWORD" value="${GOOGLE_PASSWORD}" />
        <env-var name="GOOGLE_CLIENT_ID" value="${GOOGLE_CLIENT_ID}"/>
        <env-var name="GOOGLE_CLIENT_SECRET" value="${GOOGLE_CLIENT_SECRET}"/>
        <env-var name="GITHUB_CLIENT_ID" value="${GITHUB_CLIENT_ID}"/>
        <env-var name="GITHUB_CLIENT_SECRET" value="${GITHUB_CLIENT_SECRET}"/>
        <env-var name="EMAIL_SENDER" value="${EMAIL_SENDER}"/>
        <env-var name="AUTH_SECRET_TOKEN" value="${AUTH_SECRET_TOKEN}"/>
        <env-var name="AUTH_CAPTCHA_PUBLIC" value="${AUTH_CAPTCHA_PUBLIC}"/>
        <env-var name="AUTH_CAPTCHA_SECRET" value="${AUTH_CAPTCHA_SECRET}"/>
        <env-var name="DB_HOST" value="${DB_HOST}"/>
        <env-var name="DB_NAME" value="${DB_NAME}"/>
        <env-var name="DB_USER" value="${DB_USER}"/>
        <env-var name="DB_PASS" value="${DB_PASS}"/>
    </env-variables>
</appengine-web-app>